package com.adobe.air.imagebrowser.data {		import com.adobe.air.imagebrowser.controls.Tag;		public class TagFactory {				// Constants:		// Public Properties:		// Protected Properties:		protected var tags:Vector.<Tag>			// Initialization:		public function TagFactory() {			tags = new Vector.<Tag>;		}			// Public Methods:		public function getTag():Tag {			var tag:Tag = tags.pop() as Tag;			if (tag == null) {				tag = new Tag();			}			return tag;		}				public function addTag(p_tag:Tag):void {			tags.push(p_tag);		}		// Protected Methods:	}	}