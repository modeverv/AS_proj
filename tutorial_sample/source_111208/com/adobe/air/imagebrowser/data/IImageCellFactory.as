package com.adobe.air.imagebrowser.data {		import com.adobe.air.imagebrowser.controls.IImageCell;		public class IImageCellFactory {				// Constants:		// Public Properties:		// Protected Properties:		protected var cells:Vector.<IImageCell>;		protected var _itemRenderer:IImageCell;			// Initialization:		public function IImageCellFactory(p_itemRenderer:IImageCell) {			cells = new Vector.<IImageCell>;			_itemRenderer = p_itemRenderer;		}			// Public Methods:		public function set itemRenderer(p_value:IImageCell):void {			_itemRenderer = p_value;		}						public function getCell():IImageCell {			var cell:IImageCell = cells.pop() as IImageCell;			if (cell == null) {				cell = _itemRenderer.getInstance();			}			return cell;		}				public function addCell(p_cell:IImageCell):void {			cells.push(p_cell);		}		// Protected Methods:	}	}