package com.adobe.air.imagebrowser.events {		import flash.events.Event;		public class TagEvent extends Event {				// Constants:		public static const HIGHLIGHT_PHOTOS:String = "highlightPhotos";		public static const HIGHLIGHT_TAGS:String = "highlightTags";		public static const HIGHLIGHT_ACTIVE:String = "highlightActive";		// Public Properties:		public var tags:Array;		// Private Properties:			// Initialization:		public function TagEvent(p_type:String, p_tags:Array, p_bubbles:Boolean=true) {			super(p_type, p_bubbles, false);			tags = p_tags;		}			// Public Methods:		public function get firstTag():String {			if (tags == null || tags.length == 0) { return null; }			return tags[0];		}		override public function clone():Event {			return new TagEvent(type, tags, bubbles);		}		override public function toString():String {			return formatToString("TagEvent", type, tags, bubbles, cancelable);		}		// Protected Methods:	}	}