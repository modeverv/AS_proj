<?xml version="1.0" encoding="utf-8" ?>
<application
	xmlns="http://ns.adobe.com/air/application/1.5"
        minimumPatchLevel="4">
        <id>com.adobe.air.samples.bkmark</id>
        <name>BkMark</name>
        <version>1.2</version>
        <filename>BkMark</filename>
	<description>This is a sample application that links a web page at your choice from your desktop.</description>
	<copyright>Copyright 2007-2008. Adobe Systems Incorporated. All rights reserved.</copyright>
	<initialWindow>
		<content>root.html</content>
		<title>BkMark</title>
		<systemChrome>standard</systemChrome>
		<transparent>false</transparent>
		<visible>false</visible>
		<minimizable>false</minimizable>
		<maximizable>false</maximizable>
		<resizable>false</resizable>
		<width>240</width>
		<!-- <height>360</height> -->
		<height>325</height>
		<x>150</x>
		<y>150</y>
	</initialWindow>
    <installFolder>AIR Examples</installFolder>
    <programMenuFolder>AIR Examples</programMenuFolder>
    <icon>
		<image16x16>src/icons/AIRApp_16.png</image16x16>
		<image32x32>src/icons/AIRApp_32.png</image32x32>
		<image48x48>src/icons/AIRApp_48.png</image48x48>
		<image128x128>src/icons/AIRApp_128.png</image128x128>
	</icon>
	<customUpdateUI>false</customUpdateUI>
	<allowBrowserInvocation>false</allowBrowserInvocation>
</application>